  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-alpha.45",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-load-plugins": "^1.0.0",
    "jasmine": "^2.3.2",
    "ts-node": "^0.2.4",
    "tsd": "^0.6.5",
    "typescript": "^1.6.2"
  }
}
